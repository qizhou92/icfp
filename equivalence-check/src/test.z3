(set-option :fixedpoint.engine "duality")
(declare-rel R1  (Int Bool) )
(declare-rel Goal ()) 
(declare-var a Int)
(declare-var b Bool)
(rule (=> (and (= b true) (= a 5)) (R1 a b)))
(rule (=> (and (= b false) (not (= a 5))) (R1 a b)))
(rule (=> (and (R1 a b) (not b) (= a 5) ) Goal))
(query Goal :print-certificate true)