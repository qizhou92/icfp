(set-option :fixedpoint.engine "duality")
(declare-rel R1 (Int Int Int) )
(declare-rel Goal ()) 
(declare-var x Int)
(declare-var y Int)
(declare-var z Int)
(rule (=> (and (= x (+ y 1))(= z 3))  (R1 x y z)))
(rule (=> (and (= x 2) (= y 1) (>= z 3)) (R1 x y z)))
(rule (=> (and (R1 x y z) (= z 4) (= x 3) (= y 2) ) Goal))
(query Goal :print-certificate true)