(set-option :fixedpoint.engine "duality")
(declare-rel R1 (Int Int) )
(declare-rel R2 (Int Int) )
(declare-rel R3 (Int ) )
(declare-rel Goal ()) 
(declare-var x1 Int)
(declare-var x2 Int)
(declare-var x3 Int)
(declare-var x4 Int)
(rule (=> (and (= x4 x1)) (R1 x1 x4)))
(rule (=> (R1 x1 x4) (R3 x4) ))
(rule (=> (and (R3 x1) (= x1 9)) Goal))
 (query Goal :print-certificate true)